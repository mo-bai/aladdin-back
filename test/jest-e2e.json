{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testTimeout": 10000,
  "maxConcurrency": 1,
  "maxWorkers": 1,
  "verbose": true,
  "detectOpenHandles": true,
  "forceExit": true
}
